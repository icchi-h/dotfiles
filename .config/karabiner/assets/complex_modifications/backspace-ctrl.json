{
  "title": "Remap Ctrl+I to BackSpace and Ctrl+O to Delete",
  "rules": [
    {
      "description": "左Ctrl＋IキーをBackSpaceにリマップする",
      "manipulators": [
        {
          "from": {
            "key_code": "i",
            "modifiers": {
              "mandatory": [
                "left_control"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "delete_or_backspace"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "左Ctrl＋OキーをDeleteにリマップする",
      "manipulators": [
        {
          "from": {
            "key_code": "o",
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            }
          },
          "to": [
            {
              "key_code": "delete_forward"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
